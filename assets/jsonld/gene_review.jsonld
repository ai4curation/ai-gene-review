      "definition_uri": "https://w3id.org/ai4curation/gene_review/EvidenceType",
      "from_schema": "https://w3id.org/ai4curation/gene_review",
      "permissible_values": [
        {
          "text": "IDA"
        },
        {
          "text": "IBA"
        },
        {
          "text": "ISS"
        },
        {
