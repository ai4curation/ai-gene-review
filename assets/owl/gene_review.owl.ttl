    rdfs:label "taxon" ;
    rdfs:range gene_review:Term ;
    skos:inScheme <https://w3id.org/ai4curation/gene_review> .

gene_review:title a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "title" ;
    rdfs:range linkml:String ;
    skos:definition "Title of the entity" ;
    skos:inScheme <https://w3id.org/ai4curation/gene_review> .

gene_review:supported_by a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "supported_by" ;
    rdfs:range gene_review:SupportingTextInReference ;
    skos:inScheme <https://w3id.org/ai4curation/gene_review> .

gene_review:reference_section_type a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "reference_section_type" ;
    rdfs:range linkml:String ;
    skos:definition "Type of section in the reference (e.g., 'ABSTRACT', 'METHODS', 'RESULTS', 'DISCUSSION')" ;
    skos:inScheme <https://w3id.org/ai4curation/gene_review> .

gene_review:supporting_text a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "supporting_text" ;
    rdfs:range linkml:String ;
    skos:definition "Supporting text from the publication. This should be exact substrings. Different substrings can be broken up by '...'s. These substrings will be checked against the actual text of the paper. If editorialization is necessary, put this in square brackets (this is not checked). For example, you can say '...[CFAP300 shows] transport within cilia is IFT dependent...'" ;
    skos:inScheme <https://w3id.org/ai4curation/gene_review> .

gene_review:term a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "term" ;
    rdfs:range gene_review:Term ;
    skos:definition "Term to be annotated" ;
    skos:inScheme <https://w3id.org/ai4curation/gene_review> .

gene_review:Reference a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Reference" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom gene_review:Finding ;
            owl:onProperty gene_review:findings ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:findings ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:title ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty gene_review:title ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty gene_review:id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty gene_review:title ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:is_invalid ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty gene_review:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:is_invalid ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Boolean ;
            owl:onProperty gene_review:is_invalid ] ;
    skos:definition "A reference is a published text  that describes a finding or a method. References might be formal publications (where the ID is a PMID), or for methods, a GO_REF. Additionally, a reference to a local ad-hoc analysis or review can be made by using the `file:` prefix." ;
    skos:inScheme <https://w3id.org/ai4curation/gene_review> .

gene_review:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    skos:definition "Description of the core function",
        "Description of the entity" ;
    skos:inScheme <https://w3id.org/ai4curation/gene_review> .

gene_review:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    skos:inScheme <https://w3id.org/ai4curation/gene_review> .

gene_review:EvidenceType a owl:Class,
        linkml:EnumDefinition ;
    owl:unionOf ( <https://w3id.org/ai4curation/gene_review/EvidenceType#IDA> <https://w3id.org/ai4curation/gene_review/EvidenceType#IBA> <https://w3id.org/ai4curation/gene_review/EvidenceType#ISS> <https://w3id.org/ai4curation/gene_review/EvidenceType#TAS> <https://w3id.org/ai4curation/gene_review/EvidenceType#IEP> <https://w3id.org/ai4curation/gene_review/EvidenceType#IGC> ) ;
    linkml:permissible_values <https://w3id.org/ai4curation/gene_review/EvidenceType#IBA>,
        <https://w3id.org/ai4curation/gene_review/EvidenceType#IDA>,
        <https://w3id.org/ai4curation/gene_review/EvidenceType#IEP>,
        <https://w3id.org/ai4curation/gene_review/EvidenceType#IGC>,
        <https://w3id.org/ai4curation/gene_review/EvidenceType#ISS>,
        <https://w3id.org/ai4curation/gene_review/EvidenceType#TAS> .

gene_review:Term a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Term" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:label ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty gene_review:ontology ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:ontology ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty gene_review:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty gene_review:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty gene_review:label ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:description ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty gene_review:label ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:ontology ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty gene_review:description ] ;
    skos:definition "A term in a specific ontology" ;
    skos:inScheme <https://w3id.org/ai4curation/gene_review> .

gene_review:ActionEnum a owl:Class,
        linkml:EnumDefinition ;
    owl:unionOf ( <https://w3id.org/ai4curation/gene_review/ActionEnum#ACCEPT> <https://w3id.org/ai4curation/gene_review/ActionEnum#KEEP_AS_NON_CORE> <https://w3id.org/ai4curation/gene_review/ActionEnum#REMOVE> <https://w3id.org/ai4curation/gene_review/ActionEnum#MODIFY> <https://w3id.org/ai4curation/gene_review/ActionEnum#MARK_AS_OVER_ANNOTATED> <https://w3id.org/ai4curation/gene_review/ActionEnum#UNDECIDED> <https://w3id.org/ai4curation/gene_review/ActionEnum#PENDING> ) ;
    linkml:permissible_values <https://w3id.org/ai4curation/gene_review/ActionEnum#ACCEPT>,
        <https://w3id.org/ai4curation/gene_review/ActionEnum#KEEP_AS_NON_CORE>,
        <https://w3id.org/ai4curation/gene_review/ActionEnum#MARK_AS_OVER_ANNOTATED>,
        <https://w3id.org/ai4curation/gene_review/ActionEnum#MODIFY>,
        <https://w3id.org/ai4curation/gene_review/ActionEnum#PENDING>,
        <https://w3id.org/ai4curation/gene_review/ActionEnum#REMOVE>,
        <https://w3id.org/ai4curation/gene_review/ActionEnum#UNDECIDED> .

<https://w3id.org/ai4curation/gene_review> a owl:Ontology ;
    rdfs:label "gene_curation" ;
    skos:definition "Schema for gene curation Top level entity is a GeneReview, which is about a single gene (and its equivalent swiss-prot entry). It contains a high level summary of the gene, plus a review of all existing annotations. It also contains a list of core functions, which are GO-CAM-like annotons describing the core evolved functions of the gene." .

