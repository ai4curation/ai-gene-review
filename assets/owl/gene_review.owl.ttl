            owl:allValuesFrom linkml:String ;
            owl:onProperty gene_review:supporting_text ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:statement ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:statement ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:supporting_text ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:reference_section_type ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty gene_review:statement ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty gene_review:reference_section_type ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:supporting_text ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:reference_section_type ] ;
