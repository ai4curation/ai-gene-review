gene_review:ExistingAnnotation a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ExistingAnnotation" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty gene_review:supporting_entities ],
        [ a owl:Restriction ;
            owl:allValuesFrom gene_review:Reference ;
            owl:onProperty gene_review:original_reference_id ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Boolean ;
            owl:onProperty gene_review:negated ],
        [ a owl:Restriction ;
            owl:allValuesFrom gene_review:AnnotationExtension ;
            owl:onProperty gene_review:extensions ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:review ],
        [ a owl:Restriction ;
            owl:allValuesFrom gene_review:Term ;
            owl:onProperty gene_review:term ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:original_reference_id ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty gene_review:evidence_type ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:supporting_entities ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:evidence_type ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:term ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:term ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:review ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:extensions ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty gene_review:negated ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:negated ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:evidence_type ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty gene_review:original_reference_id ],
        [ a owl:Restriction ;
            owl:allValuesFrom gene_review:Review ;
            owl:onProperty gene_review:review ] ;
    skos:definition "An existing annotation from the GO database, plus a review of the annotation." ;
    skos:inScheme <https://w3id.org/ai4curation/gene_review> .

gene_review:Finding a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Finding" ;
