<RuleConditionSet> CLOSED {
    (  $<RuleConditionSet_tes> (  <number> @linkml:Integer ;
          <conditions> @<RuleCondition> + ;
          <notes> @linkml:String ? ;
          <pairwise_overlap> @<PairwiseOverlap> *
       ) ;
       rdf:type [ <RuleConditionSet> ] ?
    )
}

<RuleGOAnnotation> CLOSED {
    (  $<RuleGOAnnotation_tes> (  <go_id> @linkml:String ;
          <go_label> @linkml:String ? ;
          <aspect> @linkml:String ?
       ) ;
       rdf:type [ <RuleGOAnnotation> ] ?
    )
}

<RuleReview> CLOSED {
    (  $<RuleReview_tes> (  dcterms:description @linkml:String ? ;
          <references> @<Reference> * ;
          <status> [ <https://w3id.org/ai4curation/gene_review/RuleReviewStatusEnum#PENDING>
             <https://w3id.org/ai4curation/gene_review/RuleReviewStatusEnum#IN_PROGRESS>
             <https://w3id.org/ai4curation/gene_review/RuleReviewStatusEnum#COMPLETE> ] ? ;
          <rule_type> [ <https://w3id.org/ai4curation/gene_review/RuleTypeEnum#ARBA>
             <https://w3id.org/ai4curation/gene_review/RuleTypeEnum#UNIRULE> ] ;
          <rule> @<EmbeddedRule> ;
          <review_summary> @linkml:String ? ;
          <action> [ <https://w3id.org/ai4curation/gene_review/RuleActionEnum#ACCEPT>
             <https://w3id.org/ai4curation/gene_review/RuleActionEnum#MODIFY>
             <https://w3id.org/ai4curation/gene_review/RuleActionEnum#DEPRECATE>
             <https://w3id.org/ai4curation/gene_review/RuleActionEnum#SPLIT>
             <https://w3id.org/ai4curation/gene_review/RuleActionEnum#MERGE>
             <https://w3id.org/ai4curation/gene_review/RuleActionEnum#UNDECIDED> ] ;
          <action_rationale> @linkml:String ? ;
          <suggested_modifications> @linkml:String * ;
          <parsimony> @<ParsimonyAssessment> ? ;
          <literature_support> @<LiteratureSupportAssessment> ? ;
          <condition_overlap> @<ConditionOverlapAssessment> ? ;
          <go_specificity> @<GOSpecificityAssessment> ? ;
          <taxonomic_scope> @<TaxonomicScopeAssessment> ? ;
          <confidence> @linkml:Float ? ;
          <supported_by> @<SupportingTextInReference> *
       ) ;
       rdf:type [ <RuleReview> ]
    )
}

<RuleReviewEntry> CLOSED {
    (  $<RuleReviewEntry_tes> (  <label> @linkml:String ? ;
          <type> [ <https://w3id.org/ai4curation/gene_review/EntryTypeEnum#INTERPRO>
             <https://w3id.org/ai4curation/gene_review/EntryTypeEnum#FUNFAM>
             <https://w3id.org/ai4curation/gene_review/EntryTypeEnum#PANTHER>
             <https://w3id.org/ai4curation/gene_review/EntryTypeEnum#GO_TERM> ] ;
