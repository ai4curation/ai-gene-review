          <value> @linkml:String ;
          <curie> @linkml:String ? ;
          <label> @linkml:String ? ;
          <interpro_type> [ <https://w3id.org/ai4curation/gene_review/InterProTypeEnum#FAMILY>
             <https://w3id.org/ai4curation/gene_review/InterProTypeEnum#DOMAIN>
             <https://w3id.org/ai4curation/gene_review/InterProTypeEnum#ACTIVE_SITE>
             <https://w3id.org/ai4curation/gene_review/InterProTypeEnum#BINDING_SITE>
             <https://w3id.org/ai4curation/gene_review/InterProTypeEnum#CONSERVED_SITE>
             <https://w3id.org/ai4curation/gene_review/InterProTypeEnum#REPEAT>
             <https://w3id.org/ai4curation/gene_review/InterProTypeEnum#HOMOLOGOUS_SUPERFAMILY>
             <https://w3id.org/ai4curation/gene_review/InterProTypeEnum#PTM> ] ? ;
          <negated> @linkml:Boolean ? ;
          <protein_count> @linkml:Integer ? ;
          <protein_database> [ <https://w3id.org/ai4curation/gene_review/ProteinDatabaseEnum#SWISSPROT>
             <https://w3id.org/ai4curation/gene_review/ProteinDatabaseEnum#TREMBL>
             <https://w3id.org/ai4curation/gene_review/ProteinDatabaseEnum#UNIPROT> ] ? ;
          <uniqueness_score> @linkml:Float ? ;
          <sample_proteins> @linkml:String *
       ) ;
       rdf:type [ <RuleCondition> ] ?
    )
