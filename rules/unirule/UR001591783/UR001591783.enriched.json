      {
        "annotationType": "ANNOTATION",
        "comment": {
          "commentType": "FUNCTION",
          "texts": [
            {
              "value": "Cytokine that in its homotrimeric form binds to TNFRSF1A/TNFR1, TNFRSF1B/TNFBR and TNFRSF14/HVEM. In its heterotrimeric form with LTB binds to TNFRSF3/LTBR. Lymphotoxin is produced by lymphocytes and is cytotoxic for a wide range of tumor cells in vitro and in vivo"
            }
          ]
        }
      },
      {
        "annotationType": "ANNOTATION",
        "comment": {
          "commentType": "SUBCELLULAR LOCATION",
          "subcellularLocations": [
            {
              "location": {
                "value": "Secreted"
              }
            }
          ]
        }
      },
      {
        "annotationType": "ANNOTATION",
        "comment": {
          "commentType": "SIMILARITY",
          "texts": [
            {
              "value": "Belongs to the tumor necrosis factor family"
            }
          ]
        }
      },
      {
        "annotationType": "ANNOTATION",
        "comment": {
          "commentType": "SUBUNIT",
          "texts": [
            {
              "value": "Homotrimer, and heterotrimer of either two LTB and one LTA subunits or (less prevalent) two LTA and one LTB subunits. Interacts with TNFRSF14"
            }
          ]
        }
      },
