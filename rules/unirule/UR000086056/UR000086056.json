        "comment": {
          "commentType": "PATHWAY",
          "texts": [
            {
              "value": "Glycolipid biosynthesis; 4-amino-4-deoxy-alpha-L-arabinose undecaprenyl phosphate biosynthesis; 4-amino-4-deoxy-alpha-L-arabinose undecaprenyl phosphate from UDP-4-deoxy-4-formamido-beta-L-arabinose and undecaprenyl phosphate: step 2/2"
            }
          ]
        }
