    "annotations": [
      {
        "annotationType": "ANNOTATION",
        "comment": {
          "commentType": "FUNCTION",
          "texts": [
            {
              "value": "One of the primary rRNA binding proteins, it binds directly to 16S rRNA where it nucleates assembly of the body of the 30S subunit"
            }
          ]
        }
      },
      {
        "annotationType": "ANNOTATION",
        "comment": {
          "commentType": "FUNCTION",
          "texts": [
            {
              "value": "With S5 and S12 plays an important role in translational accuracy"
            }
          ]
        }
      },
      {
        "annotationType": "ANNOTATION",
        "comment": {
          "commentType": "SIMILARITY",
          "texts": [
            {
              "value": "Belongs to the universal ribosomal protein uS4 family"
            }
          ]
        }
      },
      {
        "annotationType": "ANNOTATION",
        "comment": {
          "commentType": "SUBUNIT",
          "texts": [
            {
              "value": "Part of the 30S ribosomal subunit. Contacts protein S5. The interaction surface between S4 and S5 is involved in control of translational fidelity"
            }
          ]
