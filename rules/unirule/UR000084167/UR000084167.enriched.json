        "annotationType": "ANNOTATION",
        "comment": {
          "commentType": "SIMILARITY",
          "texts": [
            {
              "value": "Belongs to the NtaA/SnaA/DszA monooxygenase family. RutA subfamily"
            }
          ]
        }
